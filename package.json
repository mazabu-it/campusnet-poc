{
	"name": "with-vercel-website",
	"version": "1.0.0",
	"description": "Website template for Payload",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "bun run next build",
		"postbuild": "bun run next-sitemap --config next-sitemap.config.cjs",
		"ci": "bun run payload migrate && bun run build",
		"dev": "bun run next dev",
		"dev:prod": "bun run rm -rf .next && bun run build && bun run start",
		"generate:importmap": "bun run payload generate:importmap",
		"generate:types": "bun run payload generate:types",
		"ii": "bun install",
		"lint": "bun run next lint",
		"lint:fix": "bun run next lint --fix",
		"payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
		"reinstall": "bun run rm -rf node_modules && bun run rm -f bun.lockb && bun install",
		"start": "bun run next start",
		"test": "bun run test:int && bun run test:e2e",
		"test:e2e": "bun run playwright test --config=playwright.config.ts",
		"test:int": "bun run vitest run --config ./vitest.config.mts",
		"setup-production": "bun run scripts/setup-production.js",
		"setup-production:local": "bun run scripts/setup-production.js --local"
	},
